@prefix : <http://www.semanticweb.org/fernan/ontologies/2025/3/untitled-ontology-3#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.semanticweb.org/fernan/ontologies/2025/3/untitled-ontology-3#> .

<http://www.semanticweb.org/fernan/ontologies/2025/3/untitled-ontology-3> rdf:type owl:Ontology .

#################################################################
#    Annotation properties
#################################################################

###  http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled
<http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled> rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2000/01/rdf-schema#sameAs
rdfs:sameAs rdf:type owl:AnnotationProperty .


#################################################################
#    Datatypes
#################################################################

###  http://www.semanticweb.org/fernan/ontologies/2025/3/untitled-ontology-3#StarRatingDataType
:StarRatingDataType rdf:type rdfs:Datatype ;
                    owl:equivalentClass [ rdf:type rdfs:Datatype ;
                                          owl:onDatatype xsd:integer ;
                                          owl:withRestrictions ( [ xsd:minInclusive 1
                                                                 ]
                                                                 [ xsd:maxInclusive 5
                                                                 ]
                                                               )
                                        ] .


###  http://www.w3.org/2001/XMLSchema#date
xsd:date rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/fernan/ontologies/2025/3/untitled-ontology-3#attractionLocatedIn
:attractionLocatedIn rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf owl:topObjectProperty ;
                     owl:inverseOf :hasAttraction ;
                     rdf:type owl:FunctionalProperty ;
                     rdfs:domain :Attraction ;
                     rdfs:range :City ;
                     rdfs:comment "Links a tourist attraction to the city in which it can be visited."@en ;
                     rdfs:label "attractionLocatedIn"@en .


###  http://www.semanticweb.org/fernan/ontologies/2025/3/untitled-ontology-3#eventOccursIn
:eventOccursIn rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf owl:topObjectProperty ;
               owl:inverseOf :hasEvent ;
               rdf:type owl:FunctionalProperty ;
               rdfs:domain :Event ;
               rdfs:range :City ;
               rdfs:comment "Links an event to the city where it takes place."@en ;
               rdfs:label "eventOccursIn"@en .


###  http://www.semanticweb.org/fernan/ontologies/2025/3/untitled-ontology-3#hasAttraction
:hasAttraction rdf:type owl:ObjectProperty ;
               rdfs:domain :City ;
               rdfs:range :Attraction ;
               rdfs:comment "Links a city to the tourist attractions located within it."@en ;
               rdfs:label "hasAttraction"@en .


###  http://www.semanticweb.org/fernan/ontologies/2025/3/untitled-ontology-3#hasEvent
:hasEvent rdf:type owl:ObjectProperty ;
          rdfs:domain :City ;
          rdfs:range :Event ;
          rdfs:comment "Links a city to the events occurring within it."@en ;
          rdfs:label "hasEvent"@en .


###  http://www.semanticweb.org/fernan/ontologies/2025/3/untitled-ontology-3#hasHotel
:hasHotel rdf:type owl:ObjectProperty ;
          owl:inverseOf :hotelLocatedIn ;
          rdfs:domain :City ;
          rdfs:range :Hotel ;
          rdfs:comment "Links a city to the hotels located within it." ;
          rdfs:label "hasHotel"@en .


###  http://www.semanticweb.org/fernan/ontologies/2025/3/untitled-ontology-3#hasRestaurant
:hasRestaurant rdf:type owl:ObjectProperty ;
               owl:inverseOf :restaurantLocatedIn ;
               rdfs:domain :City ;
               rdfs:range :Restaurant ;
               rdfs:comment "Links a city to the restaurants located within it." ;
               rdfs:label "hasRestaurant"@en .


###  http://www.semanticweb.org/fernan/ontologies/2025/3/untitled-ontology-3#hasTransport
:hasTransport rdf:type owl:ObjectProperty ;
              owl:inverseOf :transportLocatedIn ;
              rdfs:domain :City ;
              rdfs:range :Transport ;
              rdfs:comment "Links a city to the available means of transport within it."@en ;
              rdfs:label "hasTransport"@en .


###  http://www.semanticweb.org/fernan/ontologies/2025/3/untitled-ontology-3#hotelLocatedIn
:hotelLocatedIn rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf owl:topObjectProperty ;
                rdf:type owl:FunctionalProperty ;
                rdfs:domain :Hotel ;
                rdfs:range :City ;
                rdfs:comment "Links a hotel to the city in which it is situated."@en ;
                rdfs:label "hotelLocatedIn"@en .


###  http://www.semanticweb.org/fernan/ontologies/2025/3/untitled-ontology-3#offersNearbyAttraction
:offersNearbyAttraction rdf:type owl:ObjectProperty ;
                        rdfs:domain :Hotel ;
                        rdfs:range :Attraction ;
                        rdfs:comment "Relates a hotel to an attraction in the same city."@en ;
                        rdfs:label "offersNearbyAttraction"@en .


###  http://www.semanticweb.org/fernan/ontologies/2025/3/untitled-ontology-3#offersNearbyRestaurant
:offersNearbyRestaurant rdf:type owl:ObjectProperty ;
                        rdfs:domain :Hotel ;
                        rdfs:range :Restaurant ;
                        rdfs:comment "Relates a hotel to a restaurant in the same city."@en ;
                        rdfs:label "offersNearbyRestaurant"@en .


###  http://www.semanticweb.org/fernan/ontologies/2025/3/untitled-ontology-3#offersNearbyTransport
:offersNearbyTransport rdf:type owl:ObjectProperty ;
                       rdfs:domain :Hotel ;
                       rdfs:range :Transport ;
                       rdfs:comment "Relates a hotel to a transport in the same city." ;
                       rdfs:label "offersNearbyTransport"@en .


###  http://www.semanticweb.org/fernan/ontologies/2025/3/untitled-ontology-3#restaurantLocatedIn
:restaurantLocatedIn rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf owl:topObjectProperty ;
                     rdf:type owl:FunctionalProperty ;
                     rdfs:domain :Restaurant ;
                     rdfs:range :City ;
                     rdfs:comment "Links a restaurant to the city where it is located."@en ;
                     rdfs:label "restaurantLocatedIn"@en .


###  http://www.semanticweb.org/fernan/ontologies/2025/3/untitled-ontology-3#transportLocatedIn
:transportLocatedIn rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf owl:topObjectProperty ;
                    rdf:type owl:FunctionalProperty ;
                    rdfs:domain :Transport ;
                    rdfs:range :City ;
                    rdfs:comment "Links a transport option to the city where it operates."@en ;
                    rdfs:label "transportLocatedIn"@en .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/fernan/ontologies/2025/3/untitled-ontology-3#attractionAverageRating
:attractionAverageRating rdf:type owl:DatatypeProperty ;
                         rdfs:domain :Attraction ;
                         rdfs:range xsd:float ;
                         rdfs:comment "The average rating score for a tourist attraction based on visitor reviews."@en ;
                         rdfs:label "Attraction average rating"@en .


###  http://www.semanticweb.org/fernan/ontologies/2025/3/untitled-ontology-3#attractionId
:attractionId rdf:type owl:DatatypeProperty ,
                       owl:FunctionalProperty ;
              rdfs:domain :Attraction ;
              rdfs:range xsd:string ;
              rdfs:comment "The unique identifier for a tourist attraction."@en ;
              rdfs:label "Attraction ID"@en .


###  http://www.semanticweb.org/fernan/ontologies/2025/3/untitled-ontology-3#attractionName
:attractionName rdf:type owl:DatatypeProperty ,
                         owl:FunctionalProperty ;
                rdfs:domain :Attraction ;
                rdfs:range xsd:string ;
                rdfs:comment "The name of the tourist attraction."@en ;
                rdfs:label "Attraction name"@en .


###  http://www.semanticweb.org/fernan/ontologies/2025/3/untitled-ontology-3#averageCost
:averageCost rdf:type owl:DatatypeProperty ;
             rdfs:domain :Transport ;
             rdfs:range xsd:float ;
             rdfs:comment "The average cost of the service or product, expressed in the local currency."@en ;
             rdfs:label "Average cost"@en .


###  http://www.semanticweb.org/fernan/ontologies/2025/3/untitled-ontology-3#cityId
:cityId rdf:type owl:DatatypeProperty ,
                 owl:FunctionalProperty ;
        rdfs:domain :City ;
        rdfs:range xsd:string ;
        rdfs:comment "The unique identifier for a city."@en ;
        rdfs:label "City ID"@en .


###  http://www.semanticweb.org/fernan/ontologies/2025/3/untitled-ontology-3#cityName
:cityName rdf:type owl:DatatypeProperty ,
                   owl:FunctionalProperty ;
          rdfs:domain :City ;
          rdfs:range xsd:string ;
          rdfs:comment "The name of the city."@en ;
          rdfs:label "City name"@en .


###  http://www.semanticweb.org/fernan/ontologies/2025/3/untitled-ontology-3#company
:company rdf:type owl:DatatypeProperty ,
                  owl:FunctionalProperty ;
         rdfs:domain :Transport ;
         rdfs:range xsd:string ;
         rdfs:comment "The company or operator that provides the service or product."@en ;
         rdfs:label "Company"@en .


###  http://www.semanticweb.org/fernan/ontologies/2025/3/untitled-ontology-3#date
:date rdf:type owl:DatatypeProperty ,
               owl:FunctionalProperty ;
      rdfs:domain :Event ;
      rdfs:range xsd:string ;
      rdfs:comment "The date on which an event occurs."@en ;
      rdfs:label "Date"@en .


###  http://www.semanticweb.org/fernan/ontologies/2025/3/untitled-ontology-3#eventId
:eventId rdf:type owl:DatatypeProperty ,
                  owl:FunctionalProperty ;
         rdfs:domain :Event ;
         rdfs:range xsd:string ;
         rdfs:comment "The unique identifier for an event."@en ;
         rdfs:label "Event ID"@en .


###  http://www.semanticweb.org/fernan/ontologies/2025/3/untitled-ontology-3#eventName
:eventName rdf:type owl:DatatypeProperty ,
                    owl:FunctionalProperty ;
           rdfs:domain :Event ;
           rdfs:range xsd:string ;
           rdfs:comment "The name of the event."@en ;
           rdfs:label "Event name"@en .


###  http://www.semanticweb.org/fernan/ontologies/2025/3/untitled-ontology-3#hotelAverageRating
:hotelAverageRating rdf:type owl:DatatypeProperty ;
                    rdfs:domain :Hotel ;
                    rdfs:range xsd:float ;
                    rdfs:comment "The average rating score for a hotel based on guest reviews."@en ;
                    rdfs:label "Hotel average rating"@en .


###  http://www.semanticweb.org/fernan/ontologies/2025/3/untitled-ontology-3#hotelId
:hotelId rdf:type owl:DatatypeProperty ,
                  owl:FunctionalProperty ;
         rdfs:domain :Hotel ;
         rdfs:range xsd:string ;
         rdfs:comment "The unique identifier for a hotel."@en ;
         rdfs:label "Hotel ID"@en .


###  http://www.semanticweb.org/fernan/ontologies/2025/3/untitled-ontology-3#hotelName
:hotelName rdf:type owl:DatatypeProperty ,
                    owl:FunctionalProperty ;
           rdfs:domain :Hotel ;
           rdfs:range xsd:string ;
           rdfs:comment "The name of the hotel." ;
           rdfs:label "Hotel name"@en .


###  http://www.semanticweb.org/fernan/ontologies/2025/3/untitled-ontology-3#latitude
:latitude rdf:type owl:DatatypeProperty ,
                   owl:FunctionalProperty ;
          rdfs:domain :City ;
          rdfs:range xsd:double ;
          rdfs:comment "The north–south coordinate of a city, in decimal degrees."@en ;
          rdfs:label "Latitude"@en .


###  http://www.semanticweb.org/fernan/ontologies/2025/3/untitled-ontology-3#longitude
:longitude rdf:type owl:DatatypeProperty ,
                    owl:FunctionalProperty ;
           rdfs:domain :City ;
           rdfs:range xsd:double ;
           rdfs:comment "The east–west coordinate of a city, in decimal degrees."@en ;
           rdfs:label "Longitude"@en .


###  http://www.semanticweb.org/fernan/ontologies/2025/3/untitled-ontology-3#pricePerNight
:pricePerNight rdf:type owl:DatatypeProperty ,
                        owl:FunctionalProperty ;
               rdfs:domain :Hotel ;
               rdfs:range xsd:positiveInteger ;
               rdfs:comment "The nightly rate of a hotel room, in the local currency."@en ;
               rdfs:label "Price per night"@en .


###  http://www.semanticweb.org/fernan/ontologies/2025/3/untitled-ontology-3#restaurantAveragePrice
:restaurantAveragePrice rdf:type owl:DatatypeProperty ,
                                 owl:FunctionalProperty ;
                        rdfs:domain :Restaurant ;
                        rdfs:range xsd:positiveInteger ;
                        rdfs:comment "The average meal price at the restaurant, in the local currency."@en ;
                        rdfs:label "Restaurant average price"@en .


###  http://www.semanticweb.org/fernan/ontologies/2025/3/untitled-ontology-3#restaurantAverageRating
:restaurantAverageRating rdf:type owl:DatatypeProperty ,
                                  owl:FunctionalProperty ;
                         rdfs:domain :Restaurant ;
                         rdfs:range xsd:float ;
                         rdfs:comment "The average rating score for a restaurant based on user reviews."@en ;
                         rdfs:label "Restaurant average rating"@en .


###  http://www.semanticweb.org/fernan/ontologies/2025/3/untitled-ontology-3#restaurantId
:restaurantId rdf:type owl:DatatypeProperty ,
                       owl:FunctionalProperty ;
              rdfs:domain :Restaurant ;
              rdfs:range xsd:string ;
              rdfs:comment "The unique identifier for a restaurant."@en ;
              rdfs:label "Restaurant ID"@en .


###  http://www.semanticweb.org/fernan/ontologies/2025/3/untitled-ontology-3#restaurantName
:restaurantName rdf:type owl:DatatypeProperty ,
                         owl:FunctionalProperty ;
                rdfs:domain :Restaurant ;
                rdfs:range xsd:string ;
                rdfs:comment "The name of the restaurant."@en ;
                rdfs:label "Restaurant name"@en .


###  http://www.semanticweb.org/fernan/ontologies/2025/3/untitled-ontology-3#starRating
:starRating rdf:type owl:DatatypeProperty ,
                     owl:FunctionalProperty ;
            rdfs:domain :Hotel ;
            rdfs:range :StarRatingDataType ;
            rdfs:comment "The official star rating of a hotel."@en ;
            rdfs:label "Star rating"@en .


###  http://www.semanticweb.org/fernan/ontologies/2025/3/untitled-ontology-3#transportId
:transportId rdf:type owl:DatatypeProperty ,
                      owl:FunctionalProperty ;
             rdfs:domain :Transport ;
             rdfs:range xsd:string ;
             rdfs:comment "The unique identifier for a transport option."@en ;
             rdfs:label "Transport ID"@en .


#################################################################
#    Classes
#################################################################

###  http://www.semanticweb.org/fernan/ontologies/2025/3/untitled-ontology-3#AndalusianRestaurant
:AndalusianRestaurant rdf:type owl:Class ;
                      rdfs:subClassOf :Restaurant ;
                      rdfs:comment "A restaurant specializing in Andalusian cuisine from the region of Andalusia, Spain, known for tapas and olive-oil dishes."@en ;
                      rdfs:label "Andalusian restaurant"@en .


###  http://www.semanticweb.org/fernan/ontologies/2025/3/untitled-ontology-3#Attraction
:Attraction rdf:type owl:Class ;
            rdfs:subClassOf [ rdf:type owl:Restriction ;
                              owl:onProperty :attractionLocatedIn ;
                              owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                              owl:onClass :City
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :attractionAverageRating ;
                              owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                              owl:onDataRange xsd:float
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :attractionId ;
                              owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                              owl:onDataRange xsd:string
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :attractionName ;
                              owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                              owl:onDataRange xsd:string
                            ] ;
            owl:disjointUnionOf ( :BeachAttraction
                                  :CulturalAttraction
                                  :MonumentAttraction
                                  :MuseumAttraction
                                  :ParkAttraction
                                ) ;
            rdfs:comment "A place of interest visited for its cultural, historical, or natural value."@en ;
            rdfs:label "Attraction"@en ;
            rdfs:sameAs "http://schema.org/TouristAttraction"@en .


###  http://www.semanticweb.org/fernan/ontologies/2025/3/untitled-ontology-3#BeachAttraction
:BeachAttraction rdf:type owl:Class ;
                 rdfs:subClassOf :Attraction ;
                 rdfs:comment "A tourist attraction located on or beside a beach."@en ;
                 rdfs:label "Beach attraction"@en .


###  http://www.semanticweb.org/fernan/ontologies/2025/3/untitled-ontology-3#BeachHotel
:BeachHotel rdf:type owl:Class ;
            rdfs:subClassOf :Hotel ;
            rdfs:comment "A hotel located on or near a beachfront"@en ;
            rdfs:label "Beach hotel"@en .


###  http://www.semanticweb.org/fernan/ontologies/2025/3/untitled-ontology-3#Bus
:Bus rdf:type owl:Class ;
     rdfs:subClassOf :Transport ;
     rdfs:comment "A large motor vehicle carrying passengers by road as part of public transport."@en ;
     rdfs:label "Bus"@en .


###  http://www.semanticweb.org/fernan/ontologies/2025/3/untitled-ontology-3#BusinessHotel
:BusinessHotel rdf:type owl:Class ;
               rdfs:subClassOf :Hotel ;
               rdfs:comment "A hotel catering primarily to business travelers with facilities like meeting rooms."@en ;
               rdfs:label "Business hotel"@en .


###  http://www.semanticweb.org/fernan/ontologies/2025/3/untitled-ontology-3#City
:City rdf:type owl:Class ;
      rdfs:subClassOf [ rdf:type owl:Restriction ;
                        owl:onProperty :cityId ;
                        owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                        owl:onDataRange xsd:string
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :cityName ;
                        owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                        owl:onDataRange xsd:string
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :latitude ;
                        owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                        owl:onDataRange xsd:double
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :longitude ;
                        owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                        owl:onDataRange xsd:double
                      ] ;
      rdfs:comment "An urban area with defined administrative boundaries and a local government."@en ;
      rdfs:label "City"@en ;
      rdfs:sameAs "http://schema.org/City"@en .


###  http://www.semanticweb.org/fernan/ontologies/2025/3/untitled-ontology-3#CulinaryEvent
:CulinaryEvent rdf:type owl:Class ;
               rdfs:subClassOf :Event ;
               rdfs:comment "An event focused on food and cooking activities."@en ;
               rdfs:label "Culinary event"@en .


###  http://www.semanticweb.org/fernan/ontologies/2025/3/untitled-ontology-3#CulturalAttraction
:CulturalAttraction rdf:type owl:Class ;
                    rdfs:subClassOf :Attraction ;
                    rdfs:comment "A tourist attraction of cultural significance, such as theaters or festivals."@en ;
                    rdfs:label "Cultural attraction"@en .


###  http://www.semanticweb.org/fernan/ontologies/2025/3/untitled-ontology-3#CulturalEvent
:CulturalEvent rdf:type owl:Class ;
               rdfs:subClassOf :Event ;
               rdfs:comment "An event celebrating arts, heritage, or cultural traditions.An event celebrating arts, heritage, or cultural traditions."@en ;
               rdfs:label "Cultural event"@en .


###  http://www.semanticweb.org/fernan/ontologies/2025/3/untitled-ontology-3#Event
:Event rdf:type owl:Class ;
       rdfs:subClassOf [ rdf:type owl:Restriction ;
                         owl:onProperty :eventOccursIn ;
                         owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                         owl:onClass :City
                       ] ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :date ;
                         owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                         owl:onDataRange xsd:dateTime
                       ] ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :eventId ;
                         owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                         owl:onDataRange xsd:string
                       ] ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :eventName ;
                         owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                         owl:onDataRange xsd:string
                       ] ;
       owl:disjointUnionOf ( :CulinaryEvent
                             :CulturalEvent
                             :MusicEvent
                             :SportEvent
                             :TechnologicalEvent
                           ) ;
       rdfs:comment "An organized occasion such as a concert, exhibition, or festival in a city."@en ;
       rdfs:label "Event"@en ;
       rdfs:sameAs "http://schema.org/Event"@en .


###  http://www.semanticweb.org/fernan/ontologies/2025/3/untitled-ontology-3#Hotel
:Hotel rdf:type owl:Class ;
       rdfs:subClassOf [ rdf:type owl:Restriction ;
                         owl:onProperty :hotelLocatedIn ;
                         owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                         owl:onClass :City
                       ] ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :hotelAverageRating ;
                         owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                         owl:onDataRange xsd:float
                       ] ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :hotelId ;
                         owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                         owl:onDataRange xsd:string
                       ] ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :hotelName ;
                         owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                         owl:onDataRange xsd:string
                       ] ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :pricePerNight ;
                         owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                         owl:onDataRange xsd:positiveInteger
                       ] ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :starRating ;
                         owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                         owl:onDataRange :StarRatingDataType
                       ] ;
       owl:disjointUnionOf ( :BeachHotel
                             :BusinessHotel
                             :LuxuryHotel
                             :RuralHotel
                             :TouristHotel
                           ) ;
       rdfs:comment "An establishment providing lodging, meals, and other guest services."@en ;
       rdfs:label "Hotel"@en ;
       rdfs:sameAs "http://schema.org/Hotel"@en .


###  http://www.semanticweb.org/fernan/ontologies/2025/3/untitled-ontology-3#ItalianRestaurant
:ItalianRestaurant rdf:type owl:Class ;
                   rdfs:subClassOf :Restaurant ;
                   rdfs:comment "A restaurant serving traditional Italian dishes such as pasta, pizza, risotto, and other regional specialties."@en ;
                   rdfs:label "Italian restaurant"@en .


###  http://www.semanticweb.org/fernan/ontologies/2025/3/untitled-ontology-3#JapaneseRestaurant
:JapaneseRestaurant rdf:type owl:Class ;
                    rdfs:subClassOf :Restaurant ;
                    rdfs:comment "A restaurant offering Japanese cuisine, including sushi, sashimi, ramen, tempura, and other classic Japanese dishes."@en ;
                    rdfs:label "Japanese restaurant"@en .


###  http://www.semanticweb.org/fernan/ontologies/2025/3/untitled-ontology-3#LuxuryHotel
:LuxuryHotel rdf:type owl:Class ;
             rdfs:subClassOf :Hotel ;
             rdfs:comment "A hotel offering upscale amenities and high-end services."@en ;
             rdfs:label "Luxury hotel"@en .


###  http://www.semanticweb.org/fernan/ontologies/2025/3/untitled-ontology-3#MediterraneanRestaurant
:MediterraneanRestaurant rdf:type owl:Class ;
                         rdfs:subClassOf :Restaurant ;
                         rdfs:comment "A restaurant featuring Mediterranean-style cooking with olive oil, fresh vegetables, seafood, and aromatic herbs."@en ;
                         rdfs:label "Mediterranean restaurant"@en .


###  http://www.semanticweb.org/fernan/ontologies/2025/3/untitled-ontology-3#Metro
:Metro rdf:type owl:Class ;
       rdfs:subClassOf :Transport ;
       rdfs:comment "An urban rapid transit system operating by underground or elevated trains."@en ;
       rdfs:label "Metro"@en .


###  http://www.semanticweb.org/fernan/ontologies/2025/3/untitled-ontology-3#MexicanRestaurant
:MexicanRestaurant rdf:type owl:Class ;
                   rdfs:subClassOf :Restaurant ;
