###  http://www.semanticweb.org/fernan/ontologies/2025/3/untitled-ontology-3#Restaurant
:Restaurant rdf:type owl:Class ;
            rdfs:subClassOf [ rdf:type owl:Restriction ;
                              owl:onProperty :restaurantLocatedIn ;
                              owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                              owl:onClass :City
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :restaurantAveragePrice ;
                              owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                              owl:onDataRange xsd:positiveInteger
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :restaurantAverageRating ;
                              owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                              owl:onDataRange xsd:float
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :restaurantId ;
                              owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                              owl:onDataRange xsd:string
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :restaurantName ;
                              owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                              owl:onDataRange xsd:string
                            ] ;
            owl:disjointUnionOf ( :AndalusianRestaurant
                                  :ItalianRestaurant
                                  :JapaneseRestaurant
                                  :MediterraneanRestaurant
                                  :MexicanRestaurant
                                ) ;
            rdfs:comment "An establishment where meals are prepared and served to customers."@en ;
            rdfs:label "Restaurant"@en ;
            rdfs:sameAs "http://schema.org/Restaurant"@en .


