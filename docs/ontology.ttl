#################################################################
#    General axioms
#################################################################

[ rdf:type owl:AllDisjointClasses ;
  owl:members ( :AndalusianRestaurant
                :ItalianRestaurant
                :JapaneseRestaurant
                :MediterraneanRestaurant
                :MexicanRestaurant
              )
] .


[ rdf:type owl:AllDisjointClasses ;
  owl:members ( :BeachAttraction
                :CulturalAttraction
                :MonumentAttraction
                :MuseumAttraction
                :ParkAttraction
              )
] .


[ rdf:type owl:AllDisjointClasses ;
  owl:members ( :BeachHotel
                :BusinessHotel
                :LuxuryHotel
                :RuralHotel
                :TouristHotel
              )
] .


[ rdf:type owl:AllDisjointClasses ;
  owl:members ( :Bus
                :Metro
                :PublicBicycle
                :Taxi
                :Tram
              )
] .


[ rdf:type owl:AllDisjointClasses ;
  owl:members ( :CulinaryEvent
                :CulturalEvent
                :MusicEvent
                :SportEvent
                :TechnologicalEvent
              )
] .


