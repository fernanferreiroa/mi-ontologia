###  http://www.semanticweb.org/fernan/ontologies/2025/3/untitled-ontology-3#Transport
:Transport rdf:type owl:Class ;
           rdfs:subClassOf [ rdf:type owl:Restriction ;
                             owl:onProperty :transportLocatedIn ;
                             owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                             owl:onClass :City
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :averageCost ;
                             owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                             owl:onDataRange xsd:float
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :transportId ;
                             owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                             owl:onDataRange xsd:string
                           ] ;
           owl:disjointUnionOf ( :Bus
                                 :Metro
                                 :PublicBicycle
                                 :Taxi
                                 :Tram
                               ) ;
           rdfs:comment "A service or means of conveying passengers or goods from one place to another."@en ;
           rdfs:label "Transport"@en ;
           rdfs:sameAs "http://data.lirmm.fr/ontologies/passim#Service"^^xsd:anyURI .


